package com.address;

public interface AddressBookInterface {
	//상세조회 처리를 위한 추상 메소드 선언
	public AddressVO getAddressDetail(AddressVO paVO);
	//입력처리를 위한 추상 메소드 선언
	public AddressVO getAddressInsert(AddressVO paVO);
	//수정처리를 위한 추상 메소드 선언
	public AddressVO getAddressUpdate(AddressVO paVO);
	//삭제처리를 위한 추상 메소드 선언
	public AddressVO getAddressDelete(AddressVO paVO);
	//전체조회를 위한 추상 메소드 선언
	public void getAddressDVDList();
}
