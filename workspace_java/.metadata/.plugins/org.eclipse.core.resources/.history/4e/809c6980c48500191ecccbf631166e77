package com.ch8;

public class Father {
	int leg;
	private static Father instance = new Father();
	private Father() {}
	public Father(int leg) {
		this.leg=leg;
	}
	//////싱글톤 패턴////////
	public static Father getInstance() {
		if(instance==null) {
			instance = new Father();
		}
		return instance;
	}
}
