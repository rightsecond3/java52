package com.helpme3;

import java.util.List;
import java.util.Map;

import com.ch5.MemberVO;
import com.google.gson.Gson;

public class Json {
	Gson gson = new Gson();
	public String toJson(List<Map<String, Object>> jList) {
		String temp = null;
		temp = gson.toJson(jList);
		return temp;
	}
	public String VOtoJson(List<MemberVO> mVO) {
		String temp = null;
		temp = gson.toJson(mVO);
		return temp;
	}
}
