package com.DVD;

import javax.swing.JFrame;

public class DVD extends JFrame {
	// 선언부
	
	///*** 생성자 ***//
	public DVD() {
		
	}
	
	//** 화면 처리부 **//
	private void initDisplay() {
		this.setSize(700, 500);
		this.setVisible(true);
	}
	
	// *메인메소드* //
	public static void main(String[] args) {
		DVD dvd = new DVD();
		dvd.initDisplay();
	}
}
