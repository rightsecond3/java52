public class DropShip extends Unit implements Movable{

	void load() {
		
	}
	void unload() {
		
	}
	@Override
	void display() {
		System.out.println("나는 드랍쉽입니다.");
	}
	@Override
	public void move(int x, int y) {
		// TODO Auto-generated method stub
		System.out.println(x+","+y+"좌표로 이동하였다.");
	}
	@Override
	public void back() {
		// TODO Auto-generated method stub
		
	}
}
