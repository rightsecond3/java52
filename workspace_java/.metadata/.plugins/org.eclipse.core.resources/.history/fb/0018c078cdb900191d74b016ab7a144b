package com.helpme4;

public class LogicSetting {
	DaoSetting setDao = new DaoSetting();
	//
	public VOMem setting(String login_id) {
		VOMem rmVO = null;
		rmVO = setDao.setting(login_id);
		return rmVO;
	}
	//
	public void setNick(String login_id, String changeNick) {
		setDao.setNick(login_id, changeNick);
	}
	public void setStatus(String login_id, String changeStatus) {
		setDao.setStatus(login_id, changeStatus);
	}
	public void setImg(String login_id, String changeImg) {
		setDao.setImg(login_id, changeImg);
	}
}
