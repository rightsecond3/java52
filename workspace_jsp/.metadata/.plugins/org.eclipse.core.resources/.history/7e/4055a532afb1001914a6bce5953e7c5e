package com.vo;

public class SwDesignVO {
	private int    d_no     = 0; 
	private int    sub_cd   = 0; 
	private String question = null; 
	private String answer1  = null; 
	private String answer2  = null; 
	private String answer3  = null; 
	private String answer4  = null; 
	private int    d_point  = 0; 
	private int    dap      = 0;
	
	public int getD_no() {
		return d_no;
	}
	public void setD_no(int d_no) {
		this.d_no = d_no;
	}
	public int getSub_cd() {
		return sub_cd;
	}
	public void setSub_cd(int sub_cd) {
		this.sub_cd = sub_cd;
	}
	public String getQuestion() {
		return question;
	}
	public void setQuestion(String question) {
		this.question = question;
	}
	public String getAnswer1() {
		return answer1;
	}
	public void setAnswer1(String answer1) {
		this.answer1 = answer1;
	}
	public String getAnswer2() {
		return answer2;
	}
	public void setAnswer2(String answer2) {
		this.answer2 = answer2;
	}
	public String getAnswer3() {
		return answer3;
	}
	public void setAnswer3(String answer3) {
		this.answer3 = answer3;
	}
	public String getAnswer4() {
		return answer4;
	}
	public void setAnswer4(String answer4) {
		this.answer4 = answer4;
	}
	public int getD_point() {
		return d_point;
	}
	public void setD_point(int d_point) {
		this.d_point = d_point;
	}
	public int getDap() {
		return dap;
	}
	public void setDap(int dap) {
		this.dap = dap;
	} 
}
