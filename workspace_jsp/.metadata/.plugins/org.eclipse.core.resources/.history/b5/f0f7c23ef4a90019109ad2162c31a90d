package com.mvc1;

import java.util.List;

import org.apache.ibatis.session.SqlSession;
import org.apache.ibatis.session.SqlSessionFactory;
import org.apache.log4j.Logger;

import com.util.MyBatisCommonFactory;
import com.vo.ZipCodeVO;

public class ZipCodeDao {
	Logger logger = Logger.getLogger(ZipCodeDao.class);
	SqlSessionFactory ssf = null;
	
	public ZipCodeDao() {
		ssf = MyBatisCommonFactory.getSqlSessionFactory();
	}
	
	public List<ZipCodeVO> zipcodeList(String dong) {
		List<ZipCodeVO> zipcodeList = null;
		try {
			SqlSession ss = ssf.openSession();
			zVO = ss.selectList(dong);
		} catch (Exception e) {
			e.printStackTrace();
		}
		
		return null;
	}
}
